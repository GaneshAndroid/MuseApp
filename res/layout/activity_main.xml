<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MuseMainActivity" >

    <ImageView
        android:id="@+id/muse_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:src="@drawable/muse" />

    <ImageView
        android:id="@+id/muse_image_facebook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/muse_image"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/facebook_image"
        android:layout_centerHorizontal="true" />

    

    <ImageView
        android:id="@+id/muse_image_gmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/muse_image_facebook"
        android:layout_below="@+id/muse_image_facebook"
        
        
        android:src="@drawable/gmail_image" />

    
    <TextView
        android:id="@+id/tv_skip_continue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/muse_image_gmail"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:clickable="true"
        android:text="Skip and Continue"
        android:textSize="20dp"
         />

</RelativeLayout>